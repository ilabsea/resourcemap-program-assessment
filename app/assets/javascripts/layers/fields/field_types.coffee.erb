
@FIELD_TYPES =
<% Field::BaseKinds.each do |kind| %>
  '<%= kind[:name] %>':
    css_class: '<%= kind[:css_class] %>'
    small_css_class: '<%= kind[:small_css_class] %>'
<% end %>
<% Field::plugin_kinds.each do |name, kind| %>
  '<%= name %>':
    css_class:'<%= kind[:css_class] %>'
    small_css_class: '<%= kind[:small_css_class] %>'
<% end %>
